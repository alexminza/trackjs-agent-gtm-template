___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const injectScript = require('injectScript');
const queryPermission = require('queryPermission');
const copyFromWindow = require('copyFromWindow');
const callInWindow = require('callInWindow');
const logToConsole = require('logToConsole');

//data.debug = true;
const log = data.debug ? logToConsole : (() => {});
log('data =', data);

const onSuccess = () => {
  log('TrackJS Agent installed successfully');
  data.gtmOnSuccess();
};

const onFailure = () => {
  log('TrackJS Agent installation failed');
  data.gtmOnFailure();
};

const scriptDomain = data.forwardingDomain || 'cdn.trackjs.com';
const scriptUrl = 'https://' + scriptDomain + '/agent/v3/latest/t.js';

log('scriptUrl =', scriptUrl);

//region Init Options
const initOptions = {
  'token': data.token,
  'network': {
    'enabled': data.networkEnabled,
    'error': data.networkError
  },
  'console': {
    'enabled': data.consoleEnabled,
    'display': data.consoleDisplay,
    'error': data.consoleError,
    'warn': data.consoleWarn
  },
  'visitor': {
    'enabled': data.visitorEnabled
  },
  'window': {
    'enabled': data.windowEnabled,
    'promise': data.windowPromise
  }
};

if(data.forwardingDomain)
  initOptions.forwardingDomain = data.forwardingDomain;

if(data.application)
  initOptions.application = data.application;

if(data.sessionId)
  initOptions.sessionId = data.sessionId;

if(data.userId)
  initOptions.sessionId = data.userId;

log('initOptions =', initOptions);
//endregion

if(queryPermission('inject_script', scriptUrl)) {
  injectScript(scriptUrl, onScriptInjected, onFailure, scriptUrl);
} else {
  log('Permissions mismatch: inject_script');
  onFailure();
}

function onScriptInjected() {
  if(queryPermission('access_globals', 'execute', 'TrackJS.install')) {
    const installResult = callInWindow('TrackJS.install', initOptions);
    if(installResult) {
      //const version = copyFromWindow('TrackJS.version');
      //log('version =', version);

      onSuccess();
    } else {
      onFailure();
    }
  } else {
    log('Permissions mismatch: access_globals');
    onFailure();
  }
}


